{
	"compilerOptions": {
		"module": "CommonJS",
		"declaration": false,
		"noEmit": true,
		"downlevelIteration": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"inlineSourceMap": true,
		"lib": ["esnext"],
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"strict": true,
		"skipLibCheck": true,
		"sourceMap": false,
		"types": ["jest", "node", "react"]
	},
	"include": []
}
